<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>True</UseWindowsForms>
    <ApplicationIcon>Resources\icon.ico</ApplicationIcon>
    <Authors>Srgjan Ristovski</Authors>
    <Description>Quickest alarm set ever :) (for Windows)</Description>
    <RepositoryUrl>https://github.com/SrgjanX/AlarmShell</RepositoryUrl>
    <Copyright>Copyright (c) Srgjan Ristovski</Copyright>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="Resources\icon.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>